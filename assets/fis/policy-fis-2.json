
{
  "Version":"2012-10-17",		 	 	 
  "Statement": [
      {
          "Sid": "AllowFisToWriteAndDeleteFaultConfigurations",
          "Effect": "Allow",
          "Action": [
              "s3:PutObject",
              "s3:DeleteObject"
          ],
          "Resource": "arn:aws:s3:::908671954593-eliezer-fis-us-east-2/fis-config/*"
      },
      {
          "Sid": "AllowFisToInspectLambdaFunctions",
          "Effect": "Allow",
          "Action": [
              "lambda:GetFunction"
          ],
          "Resource": "*"
      },
      {
          "Sid": "AllowFisToDoTagLookups",
          "Effect": "Allow",
          "Action": [
              "tag:GetResources"
          ],
          "Resource": "*"
      }
  ]
}
